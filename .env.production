# API Configuration
REACT_APP_API_URL=https://api.yourdomain.com/api
REACT_APP_WS_URL=wss://api.yourdomain.com

# Feature Flags
REACT_APP_ENABLE_WEBSOCKET=true
REACT_APP_ENABLE_ANALYTICS=true

# Authentication
REACT_APP_JWT_EXPIRY=3600
REACT_APP_REFRESH_TOKEN_EXPIRY=604800

# MLM Configuration
REACT_APP_MAX_NETWORK_DEPTH=5
REACT_APP_ENABLE_MATRIX_VIEW=true
REACT_APP_ENABLE_TREE_VIEW=true

# Business Volume Settings
REACT_APP_MIN_INVESTMENT=100
REACT_APP_MAX_INVESTMENT=10000
REACT_APP_MIN_WITHDRAWAL=50

# Commission Settings
REACT_APP_DIRECT_REFERRAL_BONUS=10
REACT_APP_LEVEL_COMMISSION_RATES="10,5,3,2,1"
REACT_APP_MATCHING_BONUS=5

# System Settings
REACT_APP_PAGINATION_LIMIT=50
REACT_APP_CACHE_DURATION=300
REACT_APP_API_TIMEOUT=30000

# Analytics and Monitoring
REACT_APP_SENTRY_DSN=your_sentry_dsn
REACT_APP_GA_TRACKING_ID=your_ga_tracking_id

# Security
REACT_APP_ENABLE_RATE_LIMITING=true
REACT_APP_MAX_LOGIN_ATTEMPTS=5
REACT_APP_LOCKOUT_DURATION=900 